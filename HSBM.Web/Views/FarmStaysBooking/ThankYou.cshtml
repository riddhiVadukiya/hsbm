                <div class="ty-col">
                    <div class="ty-lbl">Order ID</div>
                    <div class="ty-vl">@Model.OrderNo</div>
                </div>
                <div class="ty-col">
                    <div class="ty-lbl">Room Name</div>
                    <div class="ty-vl">@Model.Name</div>
                </div>
                <div class="ty-col">
