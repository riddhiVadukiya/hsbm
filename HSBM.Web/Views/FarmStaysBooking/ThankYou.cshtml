                    <div class="ty-vl">@Model.FarmStaysName</div>
                </div>
                <div class="ty-col">
                    <div class="ty-lbl">CheckIn Date</div>
                    <div class="ty-vl">@Model.strCheckInDate</div>
                </div>
                
            </div>

            <div class="ty-row">
                <div class="ty-col">
                    <div class="ty-lbl">CheckOut Date</div>
                    <div class="ty-vl">@Model.strCheckOutDate</div>
                </div>
                <div class="ty-col">
                    <div class="ty-lbl">No. Of People</div>
                    <div class="ty-vl">@Model.NoOfPeople</div>
                </div>
                <div class="ty-col">
                    <div class="ty-lbl">Exclusive/Solo</div>
                    <div class="ty-vl">@Model.ExclusiveOrSolo</div>
                </div>
                
        
            </div>

            <div class="ty-row">
                <div class="ty-col">
                    <div class="ty-lbl">Amount</div>
                    <div class="ty-vl">@Model.Amount</div>
                </div>

                <div class="ty-col">
                    <div class="ty-lbl">Net Amount</div>
                    <div class="ty-vl">@Model.NetAmount</div>
                </div>
                @if (Model.EBDAmount != null)
                    {
                    <div class="ty-col">
                        <div class="ty-lbl">Early Bird Discount</div>
                        <div class="ty-vl">₹ @Model.EBDAmount</div>
                    </div>
              }
                 @if (Model.DiscountAmount != null)
